{
  "name": "groceries",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --configuration production --aot --base-href '/groceries/'",
    "docker-build": "docker-compose build",
    "docker-start": "docker-compose up",
    "docker-test-build": "docker build -t b-grocery-list .",
    "docker-test-start": "docker run --name groceries -d -p 8080:80 b-grocery-list",
    "test": "ng test",
    "lint": "ng lint",
    "server": "http-server -p 4200 -c-1 dist/groceries",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.15",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^12.2.15",
    "@angular/compiler": "^12.2.15",
    "@angular/core": "^12.2.15",
    "@angular/forms": "^12.2.15",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "^12.2.15",
    "@angular/platform-browser-dynamic": "^12.2.15",
    "@angular/pwa": "^0.1101.2",
    "@angular/router": "^12.2.15",
    "@angular/service-worker": "^12.2.15",
    "rxjs": "~6.5.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "^12.2.15",
    "@angular/language-service": "^12.2.15",
    "@types/node": "~12.11.1",
    "codelyzer": "~6.0.1",
    "http-server": "^0.12.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
