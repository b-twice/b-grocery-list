{
  "name": "groceries",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --prod --aot --base-href '/groceries/'",
    "docker-build": "docker-compose build",
    "docker-start": "docker-compose up",
    "docker-test-build": "docker build -t b-grocery-list .",
    "docker-test-start": "docker run --name groceries -d -p 8080:80 b-grocery-list",
    "test": "ng test",
    "lint": "ng lint",
    "server": "http-server -p 4200 -c-1 dist/groceries",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/pwa": "^0.900.3",
    "@angular/router": "^10.2.4",
    "@angular/service-worker": "^10.2.4",
    "rxjs": "~6.5.4",
    "zone.js": "~0.10.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@types/node": "~12.11.1",
    "codelyzer": "~5.1.2",
    "http-server": "^0.12.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.5"
  }
}
